<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true" />
	<package name="com.sasako.action" extends="struts-default">

<action name="UserCreateAction" class="com.sasako.action.UserCreateAction" method="execute">
<result name="success">createUser.jsp</result>
</action>

<!-- UserCreateConfirmAction -->
<action name="UserCreateConfirmAction" class="com.sasako.action.UserCreateConfirmAction" method="execute">
<result name="success">createUserConfirm.jsp</result>
<result name="error">createUser.jsp</result>
</action>

 <!-- UserCreateCompleteAction -->
<action name="UserCreateCompleteAction" class="com.sasako.action.UserCreateCompleteAction" method="execute">
<result name="success">createUserComplete.jsp</result>
</action>

	</package>
</struts>